{
    "version": 2,
    "builds": [
      {
        "src": "src/index.js",
        "use": "@vercel/node",
        "config": { "maxLambdaSize": "2048mb" }

      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "src/index.js",
        "env": {
          "PUPPETEER_EXECUTABLE_PATH": "/usr/bin/chromium-browser"
        }
      }
    ]
  }
  